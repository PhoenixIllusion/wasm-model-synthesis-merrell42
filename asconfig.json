{
  "targets": {
    "debug": {
      "outFile": "./src/wasm/csp-ac-debug.wasm",
      "textFile": "./src/wasm/csp-ac-debug.wat",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "outFile": "./src/wasm/csp-ac-release.wasm",
      "textFile": "./src/wasm/csp-ac-release.wat",
      "sourceMap": false,
      "optimizeLevel": 3,
      "optimizeSpeed": true,
      "shrinkLevel": 0,
      "converge": true,
      "noAssert": false
    }
  },
  "options": {
    "runtime": "stub",
    "bindings": "raw",
    "enable": ["simd"],
    "exportRuntime": true
  }
}